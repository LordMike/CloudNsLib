{
  "version": "0.3.1",

  "title": "CloudNsLib",
  "description": "Client library for ClouDNS.net",
  "authors": [ "LordMike" ],

  "packOptions": {
    "owners": [ "LordMike" ],
    "tags": [ "ClouDNS" ],
    "projectUrl": "https://github.com/lordmike/cloudnslib",
    "licenseUrl": "https://github.com/lordmike/cloudnslib/blob/master/LICENSE.txt",
    "repository": {
      "type": "git",
      "url": "https://github.com/lordmike/cloudnslib"
    }
  },

  "buildOptions": {
    "publicSign": true,
    "keyFile": "../../SigningKey.snk"
  },

  "dependencies": {
    "Newtonsoft.Json": "9.0.1"
  },

  "frameworks": {
    "netstandard1.3": {
      "imports": "dnxcore50",
      "dependencies": {
        "System.Net.Http": "4.1.0",
        "System.Net.Primitives": "4.3.0",
        "System.Net.Sockets": "4.3.0",
        "System.Runtime.InteropServices": "4.1.0"
      }
    },
    "net45": {
      "dependencies": {
        "System.Net.Http": "4.0.0",
        "System.Net.Primitives": "4.0.0"
      }
    }
  }
}
